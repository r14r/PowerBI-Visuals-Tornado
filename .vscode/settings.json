{
  "editor.tabSize": 4,
  "editor.insertSpaces": true,
  "files.eol": "\n",
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/node_modules/**": true,
    ".tmp": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    ".tmp": false,
    "**/dist": true,
    "**/node_modules": true,
    "**/webpack.statistics.*.html": true,
    "package-lock.json": true,
    "pnpm-lock.yaml": true,
    ".vscode/*.log": true,
    "**/.tmp": true,
    "**/test.tsconfig.json": true,
    "**/test.webpack.config.js": true
  },
  "search.exclude": {
    ".tmp": true,
    "typings": true
  },
  "explorerExclude.backup": null,
}